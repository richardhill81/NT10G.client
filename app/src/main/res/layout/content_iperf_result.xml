<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dip"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    tools:showIn="@layout/content_iperftest"
    android:background="@color/gray_btn_bg_color">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="平均上行带宽"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
        <TextView android:id="@+id/textview_iperf_upbandwidth"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="最大上行带宽"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
        <TextView android:id="@+id/textview_iperf_upbandwidthpeak"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="平均下行带宽"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"   />
        <TextView android:id="@+id/textview_iperf_downbandwidth"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="最大下行带宽"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"   />
        <TextView android:id="@+id/textview_iperf_downbandwidthpeak"
            style="@style/text_black_16_singleLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip" />
    </LinearLayout>
</LinearLayout>

